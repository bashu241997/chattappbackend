{
    "version": 2,
    "name": "server-demo",
    "builds": [
      { "src": "./index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/*",
        "dest": "./index.js"
      }
    ],
    "headers": [
      {
        "source": "/*",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
       ]
      }
    ]
  }